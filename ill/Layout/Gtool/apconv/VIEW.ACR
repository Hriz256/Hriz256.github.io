# Aperture converter for Laviner env files
# $Revision:   1.0  $

NAME View
VERSION 1.0
EXTENSION env 

CUSTOM $$NO
DEFAULT_UNITS $$INCH

FORMAT_ROUND $dcode 0 $xsize
FORMAT_SQUARE $dcode 1 $xsize
FORMAT_RECT $dcode 2 $xsize $ysize
FORMAT_OBLONG $dcode 3 $xsize $ysize
FORMAT_TARGET $dcode 6 $xsize $ysize
FORMAT_THERMAL $dcode 7 $xsize $ysize
FORMAT_DIAMOND $dcode 8 $xsize $ysize
FORMAT_CUSTOM $dcode 10 $xsize $ysize

